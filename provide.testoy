func thrice(n)
    return n * 3
end

func main(tacos)
    x <- thrice(tacos)
    y <- x + 7
    print(y)
    return y
end


## define a set of test data
testdata twice_testdata provide
	 8  4
	-2 -1
	 0  0
end

## manually generated test data above
test provide_twice
	given expected, input <- twice_testdata
	expected = twice(input)
end
