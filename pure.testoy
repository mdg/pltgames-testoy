func divide(top, bottom)
    return top / bottom
end

func append(first, second)
    return first + second
end

func main(tacos)
    x <- divide(15, tacos)
    y <- x + 7
    print(y)
    return y
end

puretest divide given
	8 -> 2 -> 4
	-10 -> 5 -> 2
	21 -> -3 -> -7
end

purefail divide given
	8 -> 0
	-23 -> 0
end

puretest append given
	"tacos" -> " and burritos" -> "tacos and burritos"
end
